# Staging Environment Configuration
# Copy this to .env when testing against staging environment

# Server URLs - Staging Environment
FRONTEND_URL=http://staging.board-app.com
BACKEND_URL=http://api-staging.board-app.com

# Server Ports (if different from standard 80/443)
FRONTEND_PORT=3001
BACKEND_PORT=3000

# Automation Settings - Extended timeouts for staging
TIMEOUT=60s
BROWSER_TYPE=chromium
HEADLESS=false

# Debug Settings
DEBUG=false